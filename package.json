{
  "name": "sethifyv2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch:tailwind": "npx tailwindcss -i ./src/css/input.css -o ./_site/css/styles.css --watch",
    "build:tailwind": "npx tailwindcss build -i ./src/css/input.css -o ./_site/css/styles.css",
    "watch:eleventy": "cross-env ELEVENTY_ENV=DEV eleventy --serve",
    "build:eleventy": "cross-env ELEVENTY_ENV=DEV npx eleventy",
    "watch:cms": "npx decap-server",
    "start": "del-cli ./_site --force && npm-run-all --parallel watch:*",
    "build": "run-s build:*"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@11ty/eleventy": "github:11ty/eleventy",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@tailwindcss/typography": "^0.5.15",
    "cross-env": "^7.0.3",
    "tailwindcss": "^3.4.10"
  },
  "dependencies": {
    "del-cli": "^5.1.0",
    "npm-run-all": "^4.1.5"
  }
}
