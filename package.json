{
  "name": "sethifyv2",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch:tailwind": "tailwindcss -i ./src/assets/css/input.css -o ./_site/assets/css/styles.css --watch",
    "build:tailwind": "tailwindcss -o ./_site/assets/css/styles.css --minify",
    "watch:eleventy": "cross-env ELEVENTY_ENV=DEV eleventy --serve",
    "build:eleventy": "cross-env ELEVENTY_ENV=PROD eleventy",
    "watch:cms": "npx decap-server",
    "start": "del-cli ./_site --force && npm-run-all --parallel watch:*",
    "build": "run-s build:*"
  },
