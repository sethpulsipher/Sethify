    "@sherby/eleventy-plugin-files-minifier": "^1.1.1",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "del-cli": "^5.1.0",
    "esbuild": "0.23.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.49",
    "postcss-import": "^16.1.0",
